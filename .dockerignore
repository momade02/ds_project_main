# Exclude Git history 
.git
.gitignore

# Exclude local Python environment
venv/
__pycache__/
*.pyc

# Exclude local OS junk
.DS_Store
Thumbs.db

# Exclude your Docker files (the container doesn't need its own recipe)
Dockerfile
.dockerignore

# Do not send local environment/secrets into the Docker build context
.env
.env.*